package array;

import java.util.Arrays;

public class Kth_Largest_Element {

	static int kthLargest(int arr[] , int k) {
		Arrays.sort(arr);
		int n = arr.length;
		return arr[n-k];
	}
	
	public static void main(String[] args) {
		int arr[] = {12, 3, 5, 7, 19}  ; 
		int k = 2; 
		System.out.println(k + " nd largest Element is : " + kthLargest(arr,k));
	}

}
