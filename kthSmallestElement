package array;

import java.util.Arrays;

public class KthSmallestElement {
	
	static int kthSmallest(int arr[] , int k) {
		Arrays.sort(arr);
		return arr[k-1];
	}
	
	public static void main(String args[]) {
		int arr[] = {1,3,2,4,5};
		int k = 3;
		System.out.println(k + " Smallest element is :" + kthSmallest(arr,k));
	}
}
